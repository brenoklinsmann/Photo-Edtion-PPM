\par \hich\af2\dbch\af31505\loch\f2   297     \}
\par \hich\af2\dbch\af31505\loch\f2   298     strcat(nome, extensao);
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   299     //O n\'ed\loch\f2 \hich\f2 vel de redu\'e7\'e3\loch\f2 o desejado deve ser um n\loch\af2\dbch\af31505\hich\f2 \'fa\loch\f2 \hich\f2 
