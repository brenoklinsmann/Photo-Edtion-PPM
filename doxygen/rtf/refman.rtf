\par \hich\af2\dbch\af31505\loch\f2   186         \}
\par \hich\af2\dbch\af31505\loch\f2   187     \}
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   188     //Verificacao do \'e2\loch\f2 \hich\f2 ngulo digitado pelo usu\'e1\loch\f2 rio.
\par \hich\af2\dbch\af31505\loch\f2   189     while (1) \{
\par \hich\af2\dbch\af31505\loch\f2   190         printf("\\\hich\f2 nDigite o \'e2\loch\f2 \hich\f2 ngulo em graus para rotacionar para a esquerda (m\'fa\loch\f2 ltiplo de 90):\\n");
\par \hich\af2\dbch\af31505\loch\f2   191         scanf("%i", &ang\hich\af2\dbch\af31505\loch\f2 ulo);
\par \hich\af2\dbch\af31505\loch\f2   192         if (angulo >= 90 && angulo % 90  == 0)\{
\par \hich\af2\dbch\af31505\loch\f2   193             break;
\par \hich\af2\dbch\af31505\loch\f2   194         \} else \{
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   195             printf("N\'fa\loch\f2 \hich\f2 mero inv\'e1\loch\f2 lido. Tente novamente\\n");
\par \hich\af2\dbch\af31505\loch\f2   196         \}
\par \hich\af2\dbch\af31505\loch\f2   197     \}
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   198     //'rotacoes' recebe o n\'fa\loch\f2 \hich\f2 mero de rota\'e7\'f5\loch\f2 es para \hich\af2\dbch\af31505\loch\f2 \hich\f2 a direita que ser\'e3\loch\f2 \hich\f2 o necess\'e1\loch\f2 rias (0, 1, 2, 3).

