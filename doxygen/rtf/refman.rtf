\hich\f2 quivo de sa\'ed\loch\f2 da.
\par \hich\af2\dbch\af31505\loch\f2   202     if (rotacoes % 2 == 0) \{
\par \hich\af2\dbch\af31505\loch\f2   203         fprintf(imagem_saida, "%s\\n%i %i\\n%i\\n", id_ppm, largura, altura, qualidade);
\par \hich\af2\dbch\af31505\loch\f2   204     \} else \{
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   205     //Caso seja um n\'fa\loch\f2 \hich\f2 mero \'ed\loch\f2 \hich\f2 mpar de rota\'e7\'f5\loch\f2 \hich\f2 es, as coordenadas de largura e altura s\'e3\hich\af2\dbch\af31505\loch\f2 \hich\f2 
o impressas na ordem contr\'e1\loch\f2 \hich\f2 ria no arquivo de sa\'ed\loch\f2 da.
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   206     //Isso garante que imagens retangulares possuam as dimens\'f5\loch\f2 es corretas quando rotacionadas.
\par \hich\af2\dbch\af31505\loch\f2   207         fprintf(imagem_saida, "%s\\n%i %i\\n%i\\n", id_ppm, altura, largura, qualidade);
\par \hich\af2\dbch\af31505\loch\f2   208     \}
\par \hich\af2\dbch\af31505\loch\f2   209     for (i = 1; i <= rotacoes; i++)\{
\par \hich\af2\dbch\af31505\loch\f2 \hich\f2   210         //A matriz auxiliar recebe os valores nas posi\'e7\'f5\loch\f2 \hich\f2 es corretas para representar a rota\'e7\'e3\loch\f2 o.
\par \hich\af2\dbch\af31505\loch\f2   211         for (j = 0; j < altura; j++)\{
\par \hich\af2\dbch\af31505\loch\f2   212             for (k = 0; k < largura; k++)\{
\par \hich\af2\dbch\af31505\loch\f2   213            \hich\af2\dbch\af31505\loch\f2      imagem_aux[largura-1-k][j].R = imagem[j][k].R;
\par \hich\af2\dbch\af31505\loch\f2   214                 imagem_aux[largura-1-k][j].G = imagem[j][k].G;
\par \hich\af2\dbch\af31505\loch\f2   215                 imagem_aux[largura-1-k][j].B = imagem[j][k].B;
