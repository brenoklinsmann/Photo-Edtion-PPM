\par \hich\af2\dbch\af31505\loch\f2   334                 f\hich\af2\dbch\af31505\loch\f2 printf(imagem_saida, "\\n");
\par \hich\af2\dbch\af31505\loch\f2   335             \}
\par \hich\af2\dbch\af31505\loch\f2   336         \}       
\par \hich\af2\dbch\af31505\loch\f2   337     \}
\par \hich\af2\dbch\af31505\loch\f2   338     fclose(imagem_saida);
