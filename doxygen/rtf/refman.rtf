\par \hich\af2\dbch\af31505\loch\f2   329             mediaG /= (zoom*zoom);
\par \hich\af2\dbch\af31505\loch\f2   330             mediaB /= (zoom*zoom);
\par \hich\af2\dbch\af31505\loch\f2   \hich\af2\dbch\af31505\loch\f2 \hich\f2 331             //Escreve-se no arquivo de sa\'ed\loch\f2 \hich\f2 da ent\'e3\loch\f2 \hich\f2 o o pixel que ser\'e1\loch\f2 \hich\f2  a m\'e9\loch\f2 
dia dos pixels da matriz quadrada.
\par \hich\af2\dbch\af31505\loch\f2   332             fprintf(imagem_saida, "%i %i %i ", mediaR, mediaG, mediaB);
