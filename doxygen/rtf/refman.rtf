\par \hich\af2\dbch\af31505\loch\f2   157                 fprintf(imagem_saida, "\\n");
\par \hich\af2\dbch\af31505\loch\f2   158             \}
\par \hich\af2\dbch\af31505\loch\f2   159         \}
\par \hich\af2\dbch\af31505\loch\f2   160     \}
