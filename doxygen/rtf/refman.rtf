\par \hich\af2\dbch\af31505\loch\f2   172     /*
\par \hich\af2\dbch\af31505\loch\f2   173         Cria-se um char para acrescenta\hich\af2\dbch\af31505\loch\f2 
r ao nome do novo arquivo o filtro que foi utilizado. O 'for' sevira para zerar as posicoes que armazenam a cadeia de caracteres '.ppm' para logo apos seu termino concatenar o nome da imagem com a extensao em questao (-rot.ppm). 
\par \hich\af2\dbch\af31505\loch\f2   174     */
\par \hich\af2\dbch\af31505\loch\f2   175     cha\hich\af2\dbch\af31505\loch\f2 r extensao[8] = "-rot.ppm";
\par \hich\af2\dbch\af31505\loch\f2   176     for (i = tamanho - 4; i < tamanho; i++)\{
\par \hich\af2\dbch\af31505\loch\f2   177         nome[i] = 0;
\par \hich\af2\dbch\af31505\loch\f2   178     \}
\par \hich\af2\dbch\af31505\loch\f2   179     strcat(nome, extensao);
\par \hich\af2\dbch\af31505\loch\f2   180     //Cria-se uma matriz auxiliar para facilitar as operacoes com os pixels da imagem.
\par \hich\af2\dbch\af31505\loch\f2   181     pixels imagem_aux[dimensao][dimensao];
\par \hich\af2\dbch\af31505\loch\f2   182     //A matrix auxiliar recebe todos os pixels da imagem recebida como entrada.
